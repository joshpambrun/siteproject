<header>
<h1><%= title %></h1>
<h3><%= link_to "Home", home_path %> | <%= link_to "About", about_path %></h3>
<hr>
<%= form_tag(search_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search for a product" %>
<%= submit_tag "Search" %>
<% end %>
<p><%= link_to "Home", home_path %> | <%= link_to "About", about_path %></p>
<hr>
</header>
